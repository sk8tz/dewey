                "dnxcore50",
                "portable-net45+win8"
            ],
            "dependencies": {
                "NETStandard.Library": "1.6.0"
            }
        },
        "netstandard1.6": {
            "imports": [
                "dotnet5.6",
                "dnxcore50",
                "portable-net45+win8"
            ],
            "dependencies": {
                "NETStandard.Library": "1.6.0"
            }
        }
    }
